	<queryString><![CDATA[SELECT
     estado.descripcion,
     expediente_licencia.num_expediente,
     expediente_licencia.fecha_apertura,
     solicitud_licencia.tipo_via_afecta,
     solicitud_licencia.id_tipo_licencia,
     solicitud_licencia.nombre_via_afecta,
     solicitud_licencia.numero_via_afecta,
     dictionary.traduccion,
     persona_juridico_fisica.nombre,
     persona_juridico_fisica.apellido1,
     persona_juridico_fisica.apellido2,
     tipo_licencia.descripcion AS descripcion1
FROM
     estado,
     expediente_licencia,
     solicitud_licencia,
     dictionary,
     persona_juridico_fisica,
     domainnodes,
     tipo_licencia
WHERE
     estado.id_estado = expediente_licencia.id_estado
 AND expediente_licencia.id_solicitud = solicitud_licencia.id_solicitud
 AND solicitud_licencia.propietario = persona_juridico_fisica.id_persona
 AND solicitud_licencia.id_tipo_licencia = tipo_licencia.id_tipo_licencia
 AND domainnodes.pattern = solicitud_licencia.id_tipo_obra||''
 AND dictionary.id_vocablo = domainnodes.id_description
 AND solicitud_licencia.id_municipio = $P{id_municipio}
 AND (domainnodes.id_domain = 1127 OR domainnodes.id_domain = 132)
 AND expediente_licencia.num_expediente IN ($P!{lista_expedientes})
 AND dictionary.locale = $P{locale}]]></queryString>

