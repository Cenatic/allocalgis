	<queryString><![CDATA[select 
warning.id_warning as id,
CASE WHEN warning.description IS NULL THEN 'Sin descripcion' ELSE warning.description END as descripcion,
CASE WHEN warning.start_warning IS NULL THEN 'Sin fecha' ELSE to_char(warning.start_warning, 'DD-MM-YYYY') END as fecha_inicio,
municipios.nombreoficial as municipio,
actuation.traduccion as tipo_actuacion,
interv.traduccion as tipo_intervencion,
CASE WHEN intervention.foreseen_budget IS NULL THEN '0.00 €' ELSE intervention.foreseen_budget::text || ' €' END as presupuesto_previsto,
ROUND((intervention.work_percentage*100),2)::text || ' %' as porcentaje_obra,
CASE WHEN intervention.causes IS NULL THEN 'Sin causa asociada' ELSE intervention.causes END as causas,
CASE WHEN intervention.ended_work IS NULL THEN 'Sin fecha' ELSE to_char(intervention.ended_work, 'DD-MM-YYYY') END as fecha_fin,
intervention.priority as nivel_prioridad
from civil_work_warnings warning
inner join municipios on municipios.id_provincia::text||municipios.id_ine::text = warning.id_municipio::text
inner join civil_work_intervention intervention  on intervention.id_warning = warning.id_warning
left join (select pattern,traduccion from domainnodes,dictionary where id_domain = (select id from domains where name = 'Tipo de Actuacion') and parentdomain = (select id from domainnodes where id_domain = (select id from domains where name = 'Tipo de Actuacion') and pattern is null)
and dictionary.id_vocablo = domainnodes.id_description and dictionary.locale = $P{LOCALE}) actuation on actuation.pattern = intervention.actuation_type
left join (select pattern,traduccion from domainnodes,dictionary where id_domain = (select id from domains where name = 'Tipo de Actuacion') and parentdomain not in (select id from domainnodes where id_domain = (select id from domains where name = 'Tipo de Actuacion') and pattern is null)
and dictionary.id_vocablo = domainnodes.id_description and dictionary.locale = $P{LOCALE}) interv on interv.pattern = intervention.intervention_type
where warning.id_type = 2 and warning.id_warning::text = $P{ID_WARNING} 
and warning.id_municipio in (select id_municipio from entidades_municipios where id_entidad::text = $P{ID_ENTIDAD})
order by municipio,tipo_actuacion ,tipo_intervencion,nivel_prioridad]]></queryString>

