FROM
     "expediente_licencia" expediente_licencia,
     "solicitud_licencia" solicitud_licencia,
     "persona_juridico_fisica" persona_juridico_fisica
WHERE
     expediente_licencia.id_solicitud = solicitud_licencia.id_solicitud
