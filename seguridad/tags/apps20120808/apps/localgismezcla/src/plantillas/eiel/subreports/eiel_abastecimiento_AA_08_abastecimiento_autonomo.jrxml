	<queryString><![CDATA[SELECT     
     eiel_t_abast_au.*,
     eiel_c_nucleo_poblacion."nombre_oficial" AS nucleo,
     eiel_c_nucleo_poblacion."codentidad" AS codentidad,
     eiel_c_nucleo_poblacion."codpoblamiento" AS codpoblamiento
FROM
     eiel_t_abast_au
     LEFT JOIN "public"."eiel_c_nucleo_poblacion" eiel_c_nucleo_poblacion ON eiel_t_abast_au."codprov" = eiel_c_nucleo_poblacion."codprov"
     AND eiel_c_nucleo_poblacion."codmunic" = eiel_t_abast_au."codmunic"
     AND eiel_c_nucleo_poblacion."codentidad" = eiel_t_abast_au."codentidad"
     AND eiel_c_nucleo_poblacion."codpoblamiento" = eiel_t_abast_au."codpoblamiento"
