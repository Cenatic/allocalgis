	<queryString><![CDATA[SELECT
      count(eiel_c_abast_rd."id") AS total_tramos,
     eiel_c_abast_rd."id_municipio" AS id_municipio,
     eiel_c_abast_rd."clave" AS clave,
     eiel_c_abast_rd."codprov" AS codprov,
     eiel_c_abast_rd."codmunic" AS codmunic,
     eiel_c_abast_rd."codentidad" AS codentidad,
     eiel_c_abast_rd."codpoblamiento" AS codpoblamiento,
     eiel_c_abast_rd."tramo_rd" AS tramo_rd,
     eiel_c_abast_rd."titular" AS titular,
     eiel_c_abast_rd."gestor" AS gestor,
     eiel_c_abast_rd."estado" AS estado,
     eiel_c_abast_rd."material" AS material,
     SUM(eiel_c_abast_rd."longitud") AS longitud,
     eiel_c_abast_rd."diametro" AS diametro,
     eiel_c_abast_rd."sist_trans" AS sist_trans,
     eiel_c_abast_rd."cota_z" AS cota_z,
     eiel_c_abast_rd."obra_ejec" AS obra_ejec,
     eiel_c_abast_rd."fecha_inst" AS fecha_inst,
     eiel_c_abast_rd."observ" AS observ,
     eiel_c_abast_rd."precision_dig" AS precision_dig,
     eiel_c_nucleo_poblacion."nombre_oficial" AS nucleo,
     eiel_c_nucleo_poblacion."codentidad" AS codentidad,
     eiel_c_nucleo_poblacion."codpoblamiento" AS codpoblamiento
FROM
"public"."eiel_c_abast_rd"	
RIGHT JOIN "public"."eiel_c_nucleo_poblacion" eiel_c_nucleo_poblacion ON (eiel_c_nucleo_poblacion."codprov" = eiel_c_abast_rd."codprov"
				AND eiel_c_nucleo_poblacion."codpoblamiento" = eiel_c_abast_rd."codpoblamiento"
				AND eiel_c_nucleo_poblacion."codmunic" = eiel_c_abast_rd."codmunic"
				AND eiel_c_nucleo_poblacion."codentidad" = eiel_c_abast_rd."codentidad") 
WHERE
     eiel_c_abast_rd.revision_expirada >= 9.699999999E9 and
     eiel_c_abast_rd.id_municipio = $P{id_municipio}$P!{filtro_rd}
group by  eiel_c_abast_rd."id_municipio",
