	      <snapshots>
	        <enabled>true</enabled>
	      </snapshots>
	      <id>Apache</id>
	      <name>apache</name>
	      <url>http://mvnrepository.com/artifact/org.apache.santuario/</url>
  	</repository>
  </repositories>
</project>
