<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<artifactId>installers</artifactId>
		<groupId>com.localgis</groupId>
		<version>3.0.0</version>
		<relativePath>..</relativePath>
	</parent>
	<groupId>com.localgis.modules</groupId>
	<artifactId>InventarioModule</artifactId>
	<name>InventarioModule</name>
	<description>Metamodulo Instalacion/Actualizacion Inventario</description>
	<packaging>jar</packaging>

	<properties>
		<module.title>Modulo Cliente Inventario de LOCALGIS</module.title>
		<module.version>3.0.0</module.version>
		<module.installType>clientJNLP</module.installType>
		<module.artifact.groupId>com.localgis.jump</module.artifact.groupId>
		<module.artifact.id>localgis-apps-inventario</module.artifact.id>
		<module.artifact.version>3.0</module.artifact.version>
		<module.artifact.type>jar</module.artifact.type>
	</properties>
	<build>
		<plugins>
			<plugin>
				<groupId>com.localgis.maven.plugin</groupId>
				<artifactId>updaterPlugin</artifactId>
			</plugin>
		</plugins>
	</build>

	<dependencies>
		<dependency>
			<groupId>com.localgis.modules</groupId>
			<artifactId>InventarioServerModule</artifactId>
			<version>3.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.localgis.modules</groupId>
			<artifactId>ServerGeopistaModule</artifactId>
			<version>3.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.localgis.modules</groupId>
			<artifactId>ServerPrincipalModule</artifactId>
			<version>3.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.localgis.modules</groupId>
			<artifactId>ServerSSOModule</artifactId>
			<version>3.0.0</version>
		</dependency>
