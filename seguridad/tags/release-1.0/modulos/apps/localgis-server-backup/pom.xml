<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.localgis-jump</groupId>
  <artifactId>localgis-server-backup</artifactId>
  <version>3.0</version>
  <packaging>war</packaging>
  <name>LOCALGIS-SERVER-Backup</name>
  	<parent>
		<artifactId>localgis-parent</artifactId>
		<groupId>com.localgis</groupId>
		<version>3.0</version>
	</parent>
	<build>
	<finalName>${artifactId}</finalName>
  	<plugins>
		<plugin>
    		<artifactId>maven-site-plugin</artifactId>
    		<version>2.0</version>
    		<configuration>
    		</configuration>
    	</plugin>
        <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-compiler-plugin</artifactId>
            <version>2.3.2</version>
            <configuration>
                <target>1.6</target>
                <source>1.6</source>
            </configuration>
        </plugin>
	 		<plugin>            
			  <groupId>org.apache.maven.plugins</groupId>
			  <artifactId>maven-war-plugin</artifactId>
			  <configuration>
			    <webXml>src\main\webapp\WEB-INF\web.xml</webXml>        
			  </configuration>
			</plugin>
  	</plugins>
  </build>
  <dependencies>
	  <dependency>
	  	<groupId>com.localgis.jump</groupId>
	  	<artifactId>localgis-context</artifactId>
	  	<version>3.0</version>
	  </dependency>	  
	  <dependency>
	  	<groupId>com.localgis.jump</groupId>
	  	<artifactId>localgis-servercontext</artifactId>
	  	<version>3.0</version>
	  </dependency>	 
	  <dependency>
	  	<groupId>com.localgis.jump</groupId>
	  	<artifactId>localgis-servercontext-backup</artifactId>
	  	<version>3.0</version>
	  </dependency>
  </dependencies>
  <properties>
  	<maven.test.skip>true</maven.test.skip>
  	<maven.test.failure.ignore>true</maven.test.failure.ignore>
  </properties>
</project>