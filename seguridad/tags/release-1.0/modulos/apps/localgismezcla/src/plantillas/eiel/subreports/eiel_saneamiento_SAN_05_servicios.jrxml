	<queryString><![CDATA[SELECT
     eiel_t_saneam_serv.*,
     eiel_c_nucleo_poblacion."nombre_oficial" AS nucleo,
     eiel_c_nucleo_poblacion."codentidad" AS codentidad,
     eiel_c_nucleo_poblacion."codpoblamiento" AS codpoblamiento,
     eiel_t_nucl_encuest_2."aag_tasa" AS aag_tasa,
     eiel_t_nucl_encuest_2."aag_instal" AS aag_instal,
     eiel_t_nucl_encuest_2."aag_contad" AS aag_contad
FROM
     "public"."eiel_t_saneam_serv" eiel_t_saneam_serv INNER JOIN "public"."eiel_c_nucleo_poblacion" eiel_c_nucleo_poblacion ON (eiel_c_nucleo_poblacion."codprov" = eiel_t_saneam_serv."codprov"
	AND eiel_c_nucleo_poblacion."codpoblamiento" = eiel_t_saneam_serv."codpoblamiento"
	AND eiel_c_nucleo_poblacion."codmunic" = eiel_t_saneam_serv."codmunic"
	AND eiel_c_nucleo_poblacion."codentidad" = eiel_t_saneam_serv."codentidad")
     INNER JOIN "public"."eiel_t_nucl_encuest_2" eiel_t_nucl_encuest_2 ON eiel_t_saneam_serv."codprov" = eiel_t_nucl_encuest_2."codprov"
     AND eiel_t_nucl_encuest_2."codentidad" = eiel_t_saneam_serv."codentidad"
     AND eiel_t_nucl_encuest_2."codpoblamiento" = eiel_t_saneam_serv."codpoblamiento"
     AND eiel_t_nucl_encuest_2."codmunic" = eiel_t_saneam_serv."codmunic"
WHERE eiel_t_saneam_serv.revision_expirada >= 9.699999999E9 
AND eiel_t_nucl_encuest_2.revision_expirada >= 9.699999999E9
AND eiel_c_nucleo_poblacion.revision_expirada >= 9.699999999E9
AND eiel_t_saneam_serv."codprov"||eiel_t_saneam_serv."codmunic" = $P{id_municipio}$P!{filtro_dss}]]></queryString>

