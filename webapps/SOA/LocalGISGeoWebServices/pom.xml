	<name>LocalGISGeoWebServices</name>

	<parent>
		<groupId>com.localgis</groupId>
		<artifactId>localgis-parent</artifactId>
		<version>3.0</version>
	</parent>

	<repositories>
		<repository>
			<id>opengeo</id>
			<name>Geotools repository</name>
			<url>http://download.osgeo.org/webdav/geotools/</url>
		</repository>
	</repositories>
	<build>
		<plugins>
			<plugin>
