			<groupId>com.localgis.security</groupId>
			<artifactId>localgis-cifrado</artifactId>
			<version>3.0</version>
		</dependency>
		
		<dependency>
			<groupId>javax.mail</groupId>
			<artifactId>mail</artifactId>
			<version>1.4</version>
		</dependency>

